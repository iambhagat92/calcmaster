{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index"
        },
        {
            "source": "/((?!api/.*).*)",
            "destination": "/index.html"
        }
    ],
    "functions": {
        "api/index.ts": {
            "maxDuration": 60
        }
    }
}